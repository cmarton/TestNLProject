                    <xs:documentation xml:lang="en">Information significant to defining a rate plan.</xs:documentation>
                </xs:annotation>
                <xs:sequence minOccurs="0">
                    <xs:element name="Guarantee" type="ns1:GuaranteeType" minOccurs="0" maxOccurs="5">
